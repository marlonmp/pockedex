# https://editorconfig.org/

root = true

[*]
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf
charset = utf-8

[*.{js,json,yml,prisma,css}]
indent_size = 2

[*.json]
insert_final_newline = ignore

[*.md]
trim_trailing_whitespace = false
